//This is my current age. 
var myAge = 28;

//These are the early years. 
var earlyYears = 2;

var earlyYears = earlyYears * 10.5;

console.log(earlyYears);

// These are the later years. 
var laterYears = myAge - 2;

console.log(laterYears);

// This calculates number of your later years accounted for by dog years. 
var laterYears = laterYears * 4;


console.log(laterYears * 4);
console.log (earlyYears);
console.log (laterYears);

//My age in Dog Years 
var myAgeInDogYears = earlyYears + laterYears;

console.log(myAgeInDogYears);

//This is my name in lowercase letters. 
var myName= 'faith'
console.log (myName.toLowerCase());

console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`)

